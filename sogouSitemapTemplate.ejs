<?xml version="1.0" encoding="UTF-8"?>
<urlset> 
  <%
  var sogouUrl = config.url

  if (config.sogousitemap.url) {
    sogouUrl = config.sogousitemap.url
  }

  var url = sogouUrl + config.root;
  posts.forEach(function(post){
  if(post.categories){
%> <url>
    <loc><%- encodeURI(url + post.path) %></loc>
    <%  if(config.sogousitemap.isAutoAdapt){ %>
    <%  }%>
    <lastmod>
      <%= post.updated.toDate().toISOString().replace(/T.*$/i, "") || post.date.toDate().toISOString().replace(/T.*$/i, "") %>
    </lastmod>
    <changefreq>daily</changefreq>
    <priority>0.8</priority>
  </url><%}}) %>
</urlset>